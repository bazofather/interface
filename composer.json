{
    "name": "bazofather/interface",
    "description": "bazofather base library",
    "homepage": "https://github.com/bazofather/interface",
    "keywords": [
        "bot",
        "igap",
        "messenger",
        "bazofather",
        "botfather",
        "telegram bot"
    ],
    "support": {
        "issues": "https://github.com/bazofather/interface/issues",
        "source": "https://github.com/bazofather/interface"
    },
    "authors": [
        {
            "name": "miladimos",
            "email": "miladimos@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/http-client": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Bazofather\\Interface\\": "./src"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT"
}
